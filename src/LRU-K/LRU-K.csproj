<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<RootNamespace>LRU_K</RootNamespace>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<PropertyGroup Label="Nuget">
		<GeneratePackageOnBuild Condition=" '$(Configuration)' == 'Release' ">true</GeneratePackageOnBuild>
		<Authors>Ahmed Muhammed Youssef</Authors>
		<Version>1.0.0</Version>
		<Description>A simple implementation of an LRU-K cache, which evicts items based on their Kth most recent access time, using a custom priority queue for efficient eviction.</Description>
		<PackageId>LRUKCache</PackageId>
		<RepositoryUrl>https://github.com/Ahmed-Muhammed-Youssef/LRUK-DotNet</RepositoryUrl>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

</Project>
